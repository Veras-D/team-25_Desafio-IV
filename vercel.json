{
    "version": 2,
    "cleanUrls": true,
    "builds": [
        {
            "src": "./src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}