{
    "version": 2,
    "cleanUrls": true,
    "functions": {
      "src/index.js": {
        "memory": 3009,
        "maxDuration": 30
      },
      "src/controller/*.js": {
        "memory": 3009,
        "maxDuration": 30
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/controller/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }  