{
    "version": 2,
    "cleanUrls": true,
    "rewrites": [
      { "source": "/users", "destination": "/api/users" },
      { "source": "/login", "destination": "/api/login" },
      { "source": "/(.*)", "destination": "/api" }
    ],
    "builds": [
      { "src": "src/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/src/index.js" }
    ]
  }
  