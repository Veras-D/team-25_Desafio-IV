{
    "cleanUrls": true,
    "builds": [
        {
            "src": "./src/index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/src/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}